    z-index: 10001; 
    display: block;    
    position: fixed;          
    top: 0;
    left:500px;
    width: 320px;
    height: 64px;
    background-color: #24770a; 
    /* border:solid 1px red; */
 
}



/* =================== seacrh submit btn fix */
#mobilesearch .sf-button-btnsearch{
    top: 16px;
    margin-right: 6px;
    background: url("images/search_white.png") center center no-repeat;
    background-size: 19px 19px;
    /* border:solid 1px red !important; */
    
}

/* === close btn search input  */
#mobilesearch #close-search{ 
    z-index: 10001;       
    width:50px;
    height:68px;
    margin-top: -50px;  
    margin-left:-55px;
    background: url("images/close_search_btn.png") center center no-repeat;
    
    
}

/* === search input field */
#mobilesearch input.oo1{
    margin-top:20px;
    width:260px;
    color:#ffffff;
    background-color: rgba(36, 119, 10, 1);
